{
  "name": "ponyfm",
  "version": "1.0.0",
  "description": "A proof-of-concept Pony.fm implementation",
  "repository": "berwyn/pfm-ng2",
  "scripts": {
    "pretest": "npm run build",
    "test": "concurrently \"tsc -w\" \"karma start ./.config/karma.conf.js --single-run --browsers PhantomJS\"",
    "test:w": "karma start",
    "build": "tsc",
    "build:w": "tsc -w",
    "postinstall": "typings install",
    "lite": "lite-server",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "start": "tsc && concurrently \"npm run build:w\" \"npm run lite\" ",
    "clean": "rimraf js.map app/**/*.js*",
    "update": "npm install && typings install",
    "start:new": "webpack-dev-server --config .config/webpack.dev.js --inline --progress --port 8080",
    "build:new": "rimraf dist && webpack --config .config/webpack.prod.js --progress --profile --bail",
    "test:new": "karma start --config .config/karma.conf.js"
  },
  "author": "berwyn <berwyn.codeweaver@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "canonical-path": "0.0.2",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.19.0",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^1.0.0",
    "karma-detect-browsers": "^2.0.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-htmlfile-reporter": "^0.3.0",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-nyan-reporter": "^0.2.3",
    "karma-opera-launcher": "^0.3.0",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-safari-launcher": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-systemjs": "^0.10.0",
    "karma-typescript-preprocessor2": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "lite-server": "^1.3.1",
    "lodash": "^4.13.1",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.6.3",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.1",
    "symbol-observable": "^0.2.4",
    "ts-loader": "^0.8.2",
    "tslint": "^3.10.2",
    "typescript": "^1.7.5",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.13.0"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router-deprecated": "^2.0.0-rc.1",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  }
}
